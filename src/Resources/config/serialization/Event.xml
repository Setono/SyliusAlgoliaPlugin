<?xml version="1.0" ?>
<serializer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns="http://symfony.com/schema/dic/serializer-mapping"
            xsi:schemaLocation="http://symfony.com/schema/dic/serializer-mapping https://symfony.com/schema/dic/serializer-mapping/serializer-mapping-1.0.xsd"
>
    <class name="Setono\SyliusAlgoliaPlugin\Client\InsightsClient\Event">
        <attribute name="eventType">
            <group>setono:sylius-algolia:event</group>
        </attribute>

        <attribute name="eventName">
            <group>setono:sylius-algolia:event</group>
        </attribute>

        <attribute name="index">
            <group>setono:sylius-algolia:event</group>
        </attribute>

        <attribute name="userToken">
            <group>setono:sylius-algolia:event</group>
        </attribute>

        <attribute name="objectIds" serialized-name="objectIDs">
            <group>setono:sylius-algolia:event</group>
        </attribute>

        <attribute name="timestamp">
            <group>setono:sylius-algolia:event</group>
        </attribute>

        <attribute name="queryId" serialized-name="queryID">
            <group>setono:sylius-algolia:event</group>
        </attribute>
    </class>
</serializer>
